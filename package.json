{
  "name": "imdone-obsidian-plugin",
  "version": "1.0.5",
  "description": "An Obsidian plugin that integrates with Imdone to highlight TODO sections, provide tag autocomplete, and open cards.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian-plugin",
    "imdone",
    "todo",
    "autocomplete",
    "tags"
  ],
  "author": "Jesse Piascik",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "imdone-core": "^1.50.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
